{
    "name": "datomatic/nova-nested-form",
    "description": "A Laravel Nova package that allows you to create/update/delete nested related fields from a parent form.",
    "keywords": [
        "laravel",
        "nova",
        "nova 4",
        "nested",
        "relationship",
        "form"
    ],
    "homepage": "https://datomatic.io",
    "support": {
        "issues": "https://github.com/datomatic/laravel-nova-nested-form/issues",
        "source": "https://github.com/datomatic/laravel-nova-nested-form",
        "docs": "https://github.com/datomatic/laravel-nova-nested-form#readme"
    },
    "authors": [
        {
            "name": "Alberto Peripolli",
            "homepage": "https://datomatic.io",
            "email": "alberto@datomatic.io"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.17|^5.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Datomatic\\NestedForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datomatic\\NestedForm\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
